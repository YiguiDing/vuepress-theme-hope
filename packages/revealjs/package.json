{
  "name": "vuepress-plugin-revealjs",
  "version": "2.0.0-beta.237",
  "description": "reveal.js plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "reveal",
    "reveal.js",
    "presentation",
    "mr-hope"
  ],
  "homepage": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/packages/revealjs#readme",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/revealjs"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./client/*": "./lib/client/*",
    "./SlidePage": "./lib/client/SlidePage.js",
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,css,scss,eot,woff,ttf}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "engines": {
    "node": ">=16.19.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@mdit/plugin-uml": "^0.4.8",
    "@types/markdown-it": "^13.0.2",
    "@vuepress/core": "2.0.0-beta.67",
    "@vuepress/client": "2.0.0-beta.67",
    "@vuepress/utils": "2.0.0-beta.67",
    "@vueuse/core": "^10.4.1",
    "reveal.js": "^4.6.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "markdown-it": "^13.0.2"
  },
  "peerDependencies": {
    "sass-loader": "^13.3.2",
    "vuepress": "2.0.0-beta.67",
    "vuepress-vite": "2.0.0-beta.67",
    "vuepress-webpack": "2.0.0-beta.67"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
